    "express": "^4.18.1",
    "formik": "^2.2.9",
    "formiojs": "^4.13.12",
    "hooks": "^0.3.2",
    "mobx": "^6.5.0",
    "mobx-react-lite": "^3.3.0",
    "multiselect-react-dropdown": "^2.0.21",
    "next": "^12.1.0",
    "next-auth": "^4.2.1",
    "next-compose-plugins": "^2.2.1",
    "next-optimized-images": "^2.6.2",
    "prop-types": "^15.7.2",
    "react": "^17.0.2",
    "react-animated-css": "^1.2.1",
    "react-code-input": "^3.10.1",
    "react-currency-input-field": "^3.6.0",
    "react-data-table-component": "^7.4.4",
    "react-datepicker": "^4.3.0",
    "react-dom": "^17.0.2",
