{
  "private": true,
  "engines": {
    "node": "^12.19.0 || ^14.15.0 || ^16.13.0"
  },
  "engineStrict": true,
  "scripts": {
    "dev": "next dev",
    "dev-debug": "cross-env NODE_OPTIONS='--inspect' next dev",
    "format": "prettier --write .",
    "format-verify": "prettier --check .",
    "lint": "eslint . --fix",
    "build": "yarn && next build && next export",
    "start": "./node_modules/next/dist/bin/next start",
    "postinstall": "next build",
    "typecheck": "tsc",
    "verify": "npm run typecheck && npm run lint && npm run format-verify",
    "test": "playwright test",
    "extract:i18n": "formatjs extract '{pages,components}/**/*.{js,ts,tsx}' --ignore='**/*.d.ts' --out-file lang/extracted/en.json --id-interpolation-pattern '[sha512:contenthash:base64:6]' --format formatter.js",
    "compile:i18n": "formatjs compile-folder --ast lang/extracted lang/compiled --format formatter.js",
    "i18n": "npm run extract:i18n && npm run compile:i18n"
  },
  "dependencies": {
    "@emotion/react": "^11.6.0",
    "@emotion/styled": "^11.6.0",
